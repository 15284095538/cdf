page{
  background-color: #f5f5f5;
  color: #333;
}

.newsList .li{
 width: 630rpx;
  padding: 30rpx;
  overflow: hidden;
  background-color: #fff;
  margin: 30rpx auto 0;
  border-radius: 30rpx;
}
.newsList .li .tit{
  line-height: 80rpx;
  text-align: center;
  font-size: 30rpx;
  font-weight: bold;
}

.newsList .li .content{
  font-size: 28rpx;
  line-height: 40rpx;
}


.newsList .li .time{
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #8b8b8b;
  margin: 10rpx auto 0;
  text-align: right;
}