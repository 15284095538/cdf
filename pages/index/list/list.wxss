page{
  background-color: #f5f5f5;
  color: #333;
}

::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}

.list-top{
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
  background-color: #fff;
  border-top: 1rpx solid #e2e2e2;
  border-bottom: 1rpx solid #e2e2e2;
  font-size: 0;
}

.list-top .li{
  flex: 1;
  display: inline-block;
  font-size: 28rpx;
  text-align: center;
}

.list-top .select{
  color: #663300;
}

.layer{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 105rpx;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 9;
}

.layer .scroll-view_H{
  height: 400rpx;
}

.layer .scroll-view_H .li{
  background-color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 26rpx;
  padding-left: 80rpx;
  border-bottom: 1rpx solid #e2e2e2;
}

.ind-shopping{
  padding: 0 30rpx;
}

.ind-shopping .li{
  width: 300rpx;
  padding: 15rpx;
  background-color: #fff;
  display: inline-block;
  margin: 30rpx 0 0 0;
  font-size: 26rpx;
  position: relative;
}

.ind-shopping .li .img{
  width: 300rpx;
  height: 200rpx;
}

.ind-shopping .li .img image{
  width: 300rpx;
  height: 200rpx;
}

.ind-shopping .li:nth-child(2n-1){
  margin-right: 30rpx;
}

.ind-shopping .li .h5{
  margin-top: 10rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.ind-shopping .li .price{
  margin-top: 10rpx;
  color: #ff6363;
}

.ind-shopping .li .type{
  width: 46rpx;
  height: 51rpx;
  position: absolute;
  left: 15rpx;
  top: 15rpx;
  z-index: 1;
}

.ind-shopping .li .type image{
  width: 46rpx;
  height: 51rpx;
}