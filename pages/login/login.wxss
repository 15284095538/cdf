.logo {
  width: 290rpx;
  height: 290rpx;
  margin: 100rpx auto;
}

.logo image {
  width: 290rpx;
  height: 290rpx;
  display: block;
}

.Inputbox {
  width: 550rpx;
  border-bottom: 1rpx solid #d8d8d8;
  font-size: 26rpx;
  padding-left: 40rpx;
  margin: 10rpx auto 0;
}

.Inputbox .int {
  height: 80rpx;
  line-height: 80rpx;
}

.button{
  width: 550rpx;
  background-color: #663300;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border-radius: 30rpx;
  margin: 30rpx auto;
  color: #fff;
  font-size: 28rpx;
}

.forget {
  width: 590rpx;
  margin: 20rpx auto 0;
  text-align: right;
  font-size: 26rpx;
  color: #663300;
}

.register {
  width: 590rpx;
  margin: 0rpx auto 0;
  font-size: 26rpx;
  color: #663300;
  text-align: center;
  position: relative;
  z-index: 2;
}
